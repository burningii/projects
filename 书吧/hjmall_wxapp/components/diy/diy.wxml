<template name="topic-1">
    <scroll-view class="diy-cat-0 flex-grow-0" scrollIntoView="cat-{{param.cat_index>=2?param.cat_index-2:0}}"
                 scrollWithAnimation="true" scrollX="true" style="margin-bottom: 10rpx;height: 104rpx;"
                 wx:if="{{param.list.length>1}}">
        <view class="flex-row">
            <form bindsubmit="_formIdSubmit" class="flex-grow-0" data-bind="catBind" data-index="{{index}}"
                  data-template="{{template_index}}" id="cat-{{index}}" reportSubmit="true" wx:for="{{param.list}}"
                  wx:key="{{item.id}}">
                <button formType="submit" id="_formIdSubmit">
                    <view bindtap="catBind"
                          class="diy-cat-one-0 {{index==param.cat_index?'active':''}} flex-y-center flex-x-center">
                        <view class="text-more">{{item.name}}</view>
                    </view>
                </button>
            </form>
        </view>
    </scroll-view>
    <view class="topic-list">
        <form bindsubmit="_formIdSubmit" data-type="navigate" data-url="{{item.page_url}}" reportSubmit="true"
              wx:for="{{param.list[param.cat_index].goods_list}}" wx:key="{{item.id}}">
            <button formType="submit" id="_formIdSubmit">
                <view class="topic-one layout-{{item.layout}}">
                    <view class="flex-row" wx:if="{{item.layout==0}}">
                        <view class="flex-grow-1 flex-col">
                            <view class="flex-grow-1">
                                <view class="topic-title text-more-2">{{item.title}}</view>
                            </view>
                            <view class="flex-grow-0 font-minor fs-sm">{{item.read_count}}人浏览</view>
                        </view>
                        <view class="flex-grow-0">
                            <image class="topic-cover-pic" mode="aspectFill" src="{{item.cover_pic}}"></image>
                        </view>
                    </view>
                    <block wx:else>
                        <view class="topic-title" style="font-weight: 600">
                            <view class="text-more-2">{{item.title}}</view>
                        </view>
                        <image class="topic-cover-pic" mode="aspectFill" src="{{item.cover_pic}}"></image>
                        <view class="flex-row">
                            <view class="flex-grow-1 font-minor fs-sm">{{item.read_count}}人浏览</view>
                        </view>
                    </block>
                </view>
            </button>
        </form>
    </view>
    <view class="text-center" style="padding-top:100rpx;" wx:if="{{param.list[param.cat_index].goods_list.length==0}}">
        暂无内容
    </view>
</template>
<template name="mch-1">
    <view class="zb-index-container-wrapper">
        <view class="mch-list">
            <view class="zb-mch-list-title">
                <text>全部门店</text>
            </view>
            <view class="mch-item" wx:for="{{mch_list}}" wx:key="{{item.id}}">
                <view class="flex flex-row bob p-24 zb-bob">
                    <view class="flex-grow-0">
                        <image class="bi" src="{{item.logo}}"
                               style="width:100rpx;height:100rpx;display:block;border-radius: 50%;"></image>
                    </view>
                    <view class="flex-grow-1" style="padding: 0 24rpx">
                        <view class="text-more zb-text-more"
                              style="padding-top:2rpx;margin-bottom: 8rpx;margin-top: 28rpx;">{{item.name}}
                        </view>
                    </view>
                    <view class="flex-grow-0 flex-y-center">
                        <form bindsubmit="_formIdSubmit" data-type="navigate"
                              data-url="/pages/storeInfo/index?storeid={{item.id}}" reportSubmit="true">
                            <button formType="submit" id="_formIdSubmit">
                                <view class="mch-btn zb-mch-btn-wrapper">进入门店
                                    <image style="width: 12rpx;height: 22rpx;margin-left: 7rpx;position: relative;top: 1rpx;"
                                           src="https://api1.qibuluo.net/addons/zjhj_mall/core/web/statics/wxapp/images/icon-jiantou-r.png"/>
                                </view>
                            </button>
                        </form>
                    </view>
                </view>
                <view class="mch-goods-list">
                    <scroll-view scrollX style="width:100%;white-space: nowrap;"
                                 wx:if="{{item.goods_list&&item.goods_list.length}}">
                        <navigator class="mch-goods-item" url="{{value.page_url}}" wx:for="{{item.goods_list}}"
                                   wx:for-index="key" wx:for-item="value" wx:key="{{value.id}}">
                            <image src="{{value.cover_pic}}" style="width: 220rpx;height: 220rpx;"></image>
                            <view class="text-more fs-sm" wx:if="{{param.price==1}}">￥{{value.price}}</view>
                        </navigator>
                    </scroll-view>
                </view>
            </view>
        </view>
    </view>
</template>
<template name="shop">
    <view class="shop-list w-100">
        <view bindtap="go" class="shop-one flex-row" data-index="{{index}}" data-template="{{template_index}}"
              wx:for="{{param?param.list:list}}" wx:key="{{index}}">
            <view class="flex-grow-0 flex-y-center">
                <form bindsubmit="_formIdSubmit" data-type="{{param.open_type}}"
                      data-url="/pages/shop-detail/shop-detail?shop_id={{param.id}}" reportSubmit="true">
                    <button formType="submit" id="_formIdSubmit">
                        <image src="{{item.pic_url}}" style="width:150rpx;height:150rpx;border-radius:150rpx;"></image>
                    </button>
                </form>
            </view>
            <view class="w-100 flex-grow-1" style="margin:0 20rpx;">
                <view class="text-more" style="color:#353535;" wx:if="{{!param||param.name==1}}">{{item.name}}</view>
                <view class="fs-sm flex-row mt-20" wx:if="{{!param||param.score==1}}">
                    <view class="flex-grow-0" style="margin-right:12rpx;">评分：</view>
                    <view class="flex-grow-1">
                        <image src="{{__wxapp_img.shop.love.url}}" style="margin-right:4rpx;width:20rpx;height:18rpx;"
                               wx:if="{{idx<=item.score-1}}" wx:for="{{item.score}}" wx:for-index="idx"
                               wx:for-item="itemn" wx:key="{{index}}"></image>
                    </view>
                </view>
                <view class="fs-sm mt-20" wx:if="{{!param||param.mobile==1}}">电话：{{item.mobile}}</view>
                <view class="fs-sm mt-20" wx:if="{{item.distance}}">距离：{{item.distance==-1?'无':item.distance}}</view>
            </view>
            <view class="flex-grow-0 flex-y-center">
                <view catchtap="goto" data-index="{{index}}" data-template="{{template_index}}">
                    <view class="flex-row flex-x-center">
                        <image src="{{__wxapp_img.shop.nav_one.url}}"
                               style="width:50rpx;height:50rpx;margin-bottom:16rpx;"></image>
                    </view>
                    <view class="fs-sm flex-x-center">一键导航</view>
                </view>
            </view>
        </view>
    </view>
</template>
<template name="time">
    <view class="user-block">
        <form bindsubmit="_formIdSubmit" data-type="{{param.open_type}}" data-url="{{param.url}}" reportSubmit="true">
            <button formType="submit" id="_formIdSubmit">
                <view class="w-100" wx:if="{{param.pic_url}}">
                    <image class="w-100" mode="widthFix" src="{{param.pic_url}}" style="display:block"></image>
                </view>
                <view class="time-block">
                    <view wx:if="{{time_all['time_'+template_index].param.start_time>0}}">距离活动开始还有</view>
                    <view wx:elif="{{time_all['time_'+template_index].param.end_time>0}}">距离活动结束还有</view>
                    <view wx:else>活动已结束</view>
                    <view>{{time_all['time_'+template_index].param.time_list.content}}</view>
                    <image mode="aspectFill" src="{{param.pic_url_1}}"></image>
                </view>
            </button>
        </form>
    </view>
</template>
<template name="goods-modal">
    <view catchtouchmove="true" class="new-modal" wx:if="{{show_attr_picker&&goods}}">
        <view class="new-modal-content">
            <image bindtap="modalCloseGoods" src="{{__wxapp_img.store.close3.url}}"
                   style="position:absolute;right:0;top:0;width:72rpx;height:72rpx;"></image>
            <view class="new-modal-header text-more">{{goods.name}}</view>
            <view class="new-modal-body mb-24" style="min-height:0;padding:0 32rpx;">
                <block wx:for="{{attr_group_list}}" wx:key="{{item.id}}">
                    <view class="font-minor mb-20">{{item.attr_group_name}}</view>
                    <view class="flex-row" style="flex-wrap:wrap">
                        <view bindtap="storeAttrClick"
                              class="flex-grow-0 attr-item {{value.checked?'active':''}} {{value.attr_num_0?'attr_num_0':''}}"
                              data-group-id="{{item.attr_group_id}}" data-id="{{value.attr_id}}"
                              wx:for="{{item.attr_list}}" wx:for-index="key" wx:for-item="value" wx:key="{{value.id}}">
                            {{value.attr_name}}
                        </view>
                    </view>
                </block>
                <view class="flex-row flex-y-center bit pt-20">
                    <view class="flex-grow-1">购买数量</view>
                    <view class="flex-grow-0 attr-picker">
                        <view class="flex-row number-input-box">
                            <view bindtap="numberSub"
                                  class="flex-grow-0 flex-x-center flex-y-center number-btn number-sub {{form.number<=1?'disabled':''}}"
                                  style="margin-right:0.01rem;">-
                            </view>
                            <view class="flex-grow-0">
                                <input bindblur="numberBlur" class="flex-grow-1 number-input" min="1" step="1"
                                       style="padding:0" type="number" value="{{form.number}}"></input>
                            </view>
                            <view bindtap="numberAdd"
                                  class="flex-grow-0 flex-x-center flex-y-center number-btn number-add">+
                            </view>
                        </view>
                    </view>
                </view>
            </view>
            <view class="new-modal-footer flex-row">
                <view class="flex-y-center flex-grow-1 pl-20 red">￥{{goods.price}}</view>
                <view bindtap="modalConfirmGoods" class="flex-grow-0 flex-y-center pr-20">
                    <view class="goods-modal-btn">加入购物车</view>
                </view>
            </view>
        </view>
    </view>
</template>
<template name="cat-position-0">
    <scroll-view scrollX="{{param.list_style!=4?'false':'true'}}">
        <view class="flex-row diy-goods-list-0 flex-grow-0"
              style="{{param.list_style!=4?'flex-wrap:wrap':'overflow:auto'}};">
            <view class="flex-grow-0 diy-goods-one diy-goods-one-{{param.list_style}} flex-row diy-goods-border diy-goods-border-{{param.style}}"
                  wx:for="{{param.list[param.cat_index].goods_list}}" wx:for-index="key" wx:for-item="value"
                  wx:key="{{value.id}}">
                <view class="flex-grow-0 diy-goods-img-{{param.list_style}}"
                      style="position:relative;{{param.list_style==0&&param.per==1?'height: 466rpx;':''}}">
                    <form bindsubmit="_formIdSubmit" data-type="navigate" data-url="{{value.page_url}}"
                          reportSubmit="true">
                        <button formType="submit" id="_formIdSubmit">
                            <image class="diy-goods-img-{{param.list_style}}" lazyLoad="true"
                                   mode="{{param.fill==1?'aspectFill':'aspectFit'}}" src="{{value.pic_url}}"
                                   style="{{param.list_style==0&&param.per==1?'height: 466rpx;':''}}"></image>
                        </button>
                    </form>
                    <image class="diy-goods-mark" lazyLoad="true" src="{{param.mark_url}}"
                           wx:if="{{param.mark==1}}"></image>
                    <view class="miaosha-time flex-row flex-y-center flex-x-center miaosha-time-{{param.list_style}}"
                          wx:if="{{param.list_style!=1&&(type=='miaosha'||type=='bargain'||type=='lottery')&&param.time==1}}">
                        <view class="fs-b flex-grow-1" style="text-align:left" wx:if="{{param.list_style==0}}">
                            {{param.type_name}}
                        </view>
                        <view class="fs-sm">
                            {{time_all[type+'_'+template_index].param.list[param.cat_index].goods_list[key].time_content}}{{time_all[type+'_'+template_index].param.list[param.cat_index].goods_list[key].time_list.content_ms}}
                        </view>
                    </view>
                </view>
                <view class="flex-row flex-grow-0 diy-goods-content diy-goods-content-{{param.list_style}}">
                    <block wx:if="{{param.list_style==1&&(type=='miaosha'||type=='bargain'||type=='lottery')&&param.time==1}}">
                        <navigator class="w-100" hoverClass="none" style="text-align:left" url="{{value.page_url}}">
                            <view class="pt-24 text-more {{param.style>=2&&param.list_style!=1?'text-center':''}}"
                                  style="width:100%;height:auto;" wx:if="{{param.name==1}}">{{value.name}}
                            </view>
                        </navigator>
                        <view class="flex-row flex-y-center w-100 miaosha-time-{{param.list_style}}">
                            <view class="fs-b flex-grow-0" style="text-align:left">
                                <image src="{{__wxapp_img.lottery.time.url}}"
                                       style="width:24rpx;height:24rpx;margin-right:12rpx;"></image>
                            </view>
                            <view class="fs-sm flex-grow-0">
                                {{time_all[type+'_'+template_index].param.list[param.cat_index].goods_list[key].time_content}}
                                <text class="red">
                                    {{time_all[type+'_'+template_index].param.list[param.cat_index].goods_list[key].time_list.content_ms}}
                                </text>
                            </view>
                        </view>
                    </block>
                    <navigator class="w-100" hoverClass="none" style="text-align:left" url="{{value.page_url}}" wx:else>
                        <view style="height: 3.6em">
                            <view class="pt-24 {{param.list_style==0?'text-more':'text-more-2'}} {{param.style>=2&&param.list_style!=1?'text-center':''}}"
                                  style="width:{{param.price==0&&param.buy==1&&param.list_style==0?'calc(100% - 50rpx)':'100%'}};"
                                  wx:if="{{param.name==1}}">{{value.name}}
                            </view>
                        </view>
                    </navigator>
                    <navigator class="flex-grow-1" hoverClass="none" url="{{value.page_url}}"
                               wx:if="{{param.price==1}}">
                        <view class="pt-24 diy-goods-price flex-grow-1 flex-y-bottom {{param.style>=2?'flex-x-center':''}}"
                              wx:if="{{type=='goods'||type=='book'}}">{{value.price_content}}
                        </view>
                        <block wx:elif="{{type=='lottery'}}">
                            <view class="pt-24 diy-goods-price flex-grow-1 flex-y-bottom flex-row fs-sm {{param.style>=2?'flex-x-center':''}}"
                                  wx:if="{{param.list_style==0}}">
                                <view class="fs-b">免费</view>
                                <view class="font-default fs-sm" style="text-decoration: line-through"
                                      wx:if="{{param.style<2}}">￥{{value.original_price}}
                                </view>
                            </view>
                            <block wx:if="{{param.list_style==1}}">
                                <view class="diy-goods-price flex-grow-1 flex-y-bottom flex-row fs-sm"
                                      style="width:100%;">
                                    <view class="fs-b">免费</view>
                                </view>
                                <view class="font-default fs-sm flex-y-bottom flex-grow-1">
                                    {{value.original_price_content}}￥{{value.original_price}}
                                </view>
                            </block>
                            <block wx:if="{{param.list_style==2}}">
                                <view class="diy-goods-price flex-y-bottom flex-row fs-sm {{param.style>=2?'flex-x-center':''}}"
                                      style="width:100%;">
                                    <view class="fs-b">免费</view>
                                </view>
                                <view class="font-default fs-sm flex-y-bottom {{param.style>=2?'flex-x-center':''}}"
                                      style="width:100%;">{{value.original_price_content}}￥{{value.original_price}}
                                </view>
                            </block>
                        </block>
                        <block wx:elif="{{type=='integral'}}">
                            <view class="pt-24 diy-goods-price flex-grow-1 flex-y-bottom flex-row {{param.style>=2?'flex-x-center':''}}"
                                  wx:if="{{param.list_style==0}}">
                                <view style="font-size:11pt;">{{value.integral_content}}</view>
                            </view>
                            <block wx:if="{{param.list_style==1}}">
                                <view class="diy-goods-price flex-grow-1 flex-y-bottom flex-row" style="width:100%;">
                                    <view style="font-size:11pt;">{{value.integral_content}}</view>
                                </view>
                                <view class="font-default fs-sm flex-y-bottom flex-grow-1">
                                    {{value.original_price_content}}
                                </view>
                            </block>
                            <block wx:if="{{param.list_style==2}}">
                                <view class="diy-goods-price flex-y-bottom flex-row" style="width:100%;">
                                    <view class="" style="font-size:11pt;">{{value.integral_content}}</view>
                                </view>
                                <view class="font-default fs-sm flex-y-bottom" style="width:100%;">
                                    {{value.original_price_content}}
                                </view>
                            </block>
                        </block>
                        <block wx:else>
                            <view class="pt-24 diy-goods-price flex-grow-1 flex-y-bottom flex-row fs-sm {{param.style>=2?'flex-x-center':''}}"
                                  wx:if="{{param.list_style==0}}">
                                <view>{{value.price_content}}￥
                                    <text class="fs-b">{{value.price}}</text>
                                </view>
                                <view class="font-default fs-sm" style="text-decoration: line-through"
                                      wx:if="{{param.style<2}}">￥{{value.original_price}}
                                </view>
                            </view>
                            <block wx:if="{{param.list_style==1}}">
                                <view class="diy-goods-price flex-grow-1 flex-y-bottom flex-row fs-sm"
                                      style="width:100%;">{{value.price_content}}￥
                                    <text class="fs-b">{{value.price}}</text>
                                </view>
                                <view class="font-default fs-sm flex-y-bottom flex-grow-1">
                                    {{value.original_price_content}}￥{{value.original_price}}
                                </view>
                            </block>
                            <block wx:if="{{param.list_style==2}}">
                                <view class="diy-goods-price flex-y-bottom flex-row fs-sm {{param.style>=2?'flex-x-center':''}}"
                                      style="width:100%;">{{value.price_content}}￥
                                    <text class="fs-b">{{value.price}}</text>
                                </view>
                                <view class="font-default fs-sm flex-y-bottom {{param.style>=2?'flex-x-center':''}}"
                                      style="width:100%;">{{value.original_price_content}}￥{{value.original_price}}
                                </view>
                            </block>
                        </block>
                    </navigator>
                    <form bindsubmit="_formIdSubmit" class="flex-y-bottom" data-bind="modalShowGoods"
                          data-cat="{{param.cat_index}}" data-goods="{{key}}" data-template="{{template_index}}"
                          reportSubmit="true"
                          wx:if="{{(param.list_style!=2||type=='goods'||type=='book')&&value.is_negotiable==0&&param.buy==1&&param.list_style<3&&(param.style<2||param.list_style==1)}}">
                        <button formType="submit" id="_formIdSubmit">
                            <view class="flex-grow-0 flex"
                                  style="justify-content: flex-end;width:{{param.price==0&&(param.list_style==1||param.list_style==2)?'100%':'auto'}}">
                                <view class="text-more buy-btn-cat buy-btn-{{param.buy_list}}"
                                      wx:if="{{param.buy_list>=2}}">{{param.buy_content?param.buy_content:'购买'}}
                                </view>
                                <image lazyLoad="true" mode="aspectFill" src="{{param.buy_content}}"
                                       style="width:50rpx;height:50rpx;" wx:else></image>
                            </view>
                        </button>
                    </form>
                </view>
            </view>
        </view>
    </scroll-view>
</template>
<template name="cat-position-1">
    <scroll-view scrollIntoView="cat-{{param.cat_index}}" scrollY="true"
                 style="width:550rpx;max-height:{{WindowHeight}}px;">
        <view class="diy-goods-list-1">
            <block wx:for="{{param.list}}" wx:key="{{item.id}}">
                <view class="diy-cat-name" id="cat-{{index}}">
                    <view class="text-more" style="width:170rpx;">{{item.name}}</view>
                </view>
                <view class="diy-goods-1 flex-row" wx:for="{{item.goods_list}}" wx:for-index="key" wx:for-item="value"
                      wx:key="{{value.id}}">
                    <view class="flex-grow-1">
                        <form bindsubmit="_formIdSubmit" data-type="navigate"
                              data-url="/pages/goods/goods?id={{value.goods_id}}" reportSubmit="true">
                            <button formType="submit" id="_formIdSubmit">
                                <view class="flex-row">
                                    <view class="flex-grow-0 diy-goods-img">
                                        <image lazyLoad="true" mode="aspectFill" src="{{value.pic_url}}"></image>
                                        <image class="diy-goods-mark" lazyLoad="true" mode="aspectFill"
                                               src="{{param.mark_url}}" wx:if="{{param.mark==1}}"></image>
                                    </view>
                                    <view class="flex-grow-1 ml-24 flex-col" style="justify-content:space-between;">
                                        <view class="text-more-2" wx:if="{{param.name==1}}">{{value.name}}</view>
                                        <view class="red" style="margin-top:10rpx;" wx:if="{{param.price==1}}">
                                            ￥{{value.price}}
                                        </view>
                                    </view>
                                </view>
                            </button>
                        </form>
                    </view>
                    <view class="flex-grow-0 flex-y-bottom">
                        <form bindsubmit="_formIdSubmit" data-bind="modalShowGoods" data-cat="{{index}}"
                              data-goods="{{key}}" data-template="{{template_index}}" reportSubmit="true">
                            <button formType="submit" id="_formIdSubmit">
                                <view class="buy-btn" wx:if="{{param.buy==1}}">
                                    <view class="text-more buy-btn-{{param.buy_list}}" wx:if="{{param.buy_list>=2}}">
                                        {{param.buy_content?param.buy_content:'购买'}}
                                    </view>
                                    <image lazyLoad="true" mode="aspectFill" src="{{param.buy_content}}"
                                           wx:else></image>
                                </view>
                            </button>
                        </form>
                    </view>
                </view>
            </block>
        </view>
    </scroll-view>
</template>
<template name="diy-cat">
    <scroll-view class="diy-cat-{{param.cat_position}} flex-grow-0"
                 scrollIntoView="cat-{{param.cat_index>=2?param.cat_index-2:0}}" scrollWithAnimation="true"
                 scrollX="param.cat_position == 0" scrollY="param.cat_position == 1"
                 wx:if="{{param.list.length>1||param.cat_position==1}}">
        <view class="{{param.cat_position==0?'flex-row':''}}">
            <form bindsubmit="_formIdSubmit" class="flex-grow-0" data-bind="catBind" data-index="{{index}}"
                  data-template="{{template_index}}" id="cat-{{index}}" reportSubmit="true" wx:for="{{param.list}}"
                  wx:key="{{item.id}}">
                <button formType="submit" id="_formIdSubmit">
                    <view bindtap="catBind"
                          class="diy-cat-one-{{param.cat_position==1?2:param.cat_style}} {{index==param.cat_index?'active':''}} flex-y-center flex-x-center">
                        <view class="text-more text-center">{{item.name}}</view>
                    </view>
                </button>
            </form>
        </view>
    </scroll-view>
</template>
<template name="rubik">
    <view class="user-block m-0 rubik-list"
          style="min-height:{{param.style==0?0:param.n_minHeight?param.n_minHeight:param.new_minHeight-param.space+'rpx'}};margin:-{{param.space}}rpx;width:calc(100% + {{param.space}}rpx);height:calc(100% + {{param.space}}rpx);">
        <view class="rubik-one"
