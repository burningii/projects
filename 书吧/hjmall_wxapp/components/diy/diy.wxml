<wxs module="seatImgTransform1">
    var marginTop = function(index){
        if(index==0 || index==1){
            return 'apart-1-posi-margiTop'
        }else if(index==4 || index==5){
            return 'apart-1-posi-margiBottom'
        }else if(index==2||index==3){
           return 'apart-1-posi-margiBottom-2'
        }
    }
    var transform = function(index){
        var one=[0,2,4,6,8,9,11,13,15,17,18,20,22,24,26,27,29,31,33,35,36,38,40,42,44,45,47,49,51,53];
        var one2=[1,3,5,7,10,12,14,16,19,21,23,25,28,30,32,34,37,39,41,43,46,48,50,52];
    for(var i=0;i<one.length ;i++){
    if(index==one[i]){
        return 'zb-transform-left'
    }else{
            for(var j=0;j<one2.length ;j++){
                if(index==one2[j]) return 'zb-transform-right'
            }
        }
        }
    }
    module.exports = {
        transform: transform,
        marginTop:marginTop
    }
</wxs>
<wxs module="seatImgTransform2">
    var transform = function(index){
    if(index%2==0){
    return 'zb-transform-right'
    }else{
    return 'zb-transform-left'
    }
    }
    var transform2 = function(index){
    var one=[0,2,4,5,7,9,10,12,14,15,17,19,20,22,24]
    var one2=[1,3,6,8,11,13,16,18,21,23]
    for(var i=0;i
    <one.length
    ;i++){
    if(index==one[i]){
    return 'zb-transform-left'
    }else{
    for(var j=0;j
    <one2.length
    ;j++){
    if(index==one2[j])return 'zb-transform-right'
    }
    }
    }
    }
    var transform3 = function(index){
    var one=[0,1,2,3,8,9,10,11]
    var one2=[4,5,6,7]
    for(var i=0;i
    <one.length
    ;i++){
    if(index==one[i]){
    return 'zb-transform--180'
    }else{
    for(var j=0;j
    <one2.length
    ;j++){
    if(index==one2[j])return ''
    }
    }
    }
    }

    var transform4 = function(index){
    var one=[0,1,2,3,4]
    for(var i=0;i
    <one.length
    ;i++){
    if(index==one[i]){
    return 'zb-transform-180'
    }
    }
    }

    var transform5 = function(index){
    if(index%2==0){
    return 'zb-transform-right'
    }else{
    return 'zb-transform-left'
    }
    }
    var transform6 = function(index){
    var one=[0,1,4,5]
    for(var i=0;i
    <one.length
    ;i++){
    if(index==one[i]){
    return 'zb-transform-180'
    }
    }
    }
    var transform7 = function(index){
    var one=[0,1,2]
    for(var i=0;i
    <one.length
    ;i++){
    if(index==one[i]){
    return 'zb-transform-180'
    }
    }
    }
    module.exports = {
    transform: transform
    transform2: transform2
    transform3: transform3
    transform4: transform4
    transform5: transform5
    transform6: transform6
    transform7: transform7
    }
</wxs>
